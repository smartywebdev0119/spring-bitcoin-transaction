<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.yoghurt.crypto.transactions.client.widget">
  <ui:with field="i18n" type="com.yoghurt.crypto.transactions.client.i18n.ApplicationMessages" />
  <ui:with field="res" type="com.yoghurt.crypto.transactions.client.resources.R" />
  <ui:style>
  .textBox {
  	margin-bottom: 15px;
  }
  </ui:style>
  <g:FlowPanel>
    <g:TextBox styleName="{res.css.textBox} {style.textBox}" ui:field="hostField" />
    <g:TextBox styleName="{res.css.textBox} {style.textBox}" ui:field="portField" />
    <g:TextBox styleName="{res.css.textBox} {style.textBox}" ui:field="rpcUserField" />
    <g:TextBox styleName="{res.css.textBox} {style.textBox}" ui:field="rpcPassField" />
  </g:FlowPanel>
</ui:UiBinder>